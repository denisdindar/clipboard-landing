{
  "name": "clipboard-landing",
  "version": "1.0.0",
  "description": "Landing Page Design To Code",
  "main": "index.js",
  "scripts": {
    "watch-sass": "sass --watch scss:css",
    "compile-sass": "sass scss/main.scss css/style.css",
    "devserver": "live-server",
    "prefix-css": "postcss css/style.css --use autoprefixer -o css/style.css  --no-map",
    "start": "npm-run-all --parallel devserver watch-sass",
    "build": "npm-run-all compile-sass prefix-css"
  },
  "author": "Denis Din.",
  "license": "ISC"
}
